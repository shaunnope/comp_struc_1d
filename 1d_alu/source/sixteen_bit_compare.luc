module sixteen_bit_compare (
    input alufn[2],
    input z,
    input v,
    input n,
    output cmp[16]
  ) {

  always {
    cmp = 16x{8b0};
    
    case(alufn) {
      b00:
        cmp[0] = bx;
      b01:
        cmp[0] = z;
      b10:
        cmp[0] = n ^ v;
      b11:
        cmp[0] = (n ^ v) | z;
      default:
        cmp[0] = bx;
    }
  }
}
