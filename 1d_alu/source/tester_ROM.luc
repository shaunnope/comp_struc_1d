module tester_ROM (
	input aluout[16],
 input flags[5], 
 input alufn[6],
 input x[16],
 input y[16],

	// 
	output out[16],
 output cflags[5],
 output p 
  ) 
{
  const TESTER_ROM = {b11, b01, b01, b10, b01, b10, b10, b00};
  
  always {
	  out = TESTER_ROM[aluout];
    cflags = 5b0;
    p = x[0];
  }
}
